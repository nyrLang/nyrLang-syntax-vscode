{
	"name": "nyr",
	"displayName": "nyr",
	"description": "syntax highlighting for nyr programming language",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
